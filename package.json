{
  "name": "antistatique-frontend",
  "version": "1.1.6",
  "description": "Antistatique opinionated frontend development configuration (linters, tests,...)",
  "main": ".bin/init.js",
  "bin": {
    "antistatique-frontend": ".bin/init.js",
    "init": ".bin/init.js",
    "next": ".bin/next.js"
  },
  "repository": "https://github.com/antistatique/frontend-development-configuration",
  "author": "Antistatique",
  "license": "MIT",
  "chan": {
    "release-prefix": ""
  },
  "scripts": {
    "lint:js": "eslint --ext js,jsx,ts,tsx ./ --max-warnings=0",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "jest:start": "jest --watchAll",
    "jest:run": "jest",
    "start": "cd playground && node_modules/.bin/next dev",
    "build": "cd playground && node_modules/.bin/next build",
    "storybook:start": "cd playground && node_modules/.bin/start-storybook -p 6006",
    "storybook:build": "cd playground && node_modules/.bin/build-storybook",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-eslint": "^10.1.0",
    "cypress": "^9.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.3",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "fs-extra": "^10.0.0",
    "inquirer": "^8.2.0"
  }
}
