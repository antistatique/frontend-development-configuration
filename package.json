{
  "name": "antistatique-frontend",
  "version": "$1",
  "description": "Antistatique opinionated frontend development configuration (linters, tests,...)",
  "main": ".bin/init.js",
  "bin": {
    "antistatique-frontend": ".bin/init.js",
    "init": ".bin/init.js",
    "next": ".bin/next.js"
  },
  "repository": "https://github.com/antistatique/frontend-development-configuration",
  "author": "Antistatique",
  "license": "MIT",
  "chan": {
    "release-prefix": ""
  },
  "scripts": {
    "lint:js": "eslint --ext js,jsx,ts,tsx ./ --max-warnings=0",
    "lint:css": "stylelint 'playground/**/*.css'",
    "fix:js": "eslint --ext js,jsx,ts,tsx ./ --max-warnings=0 --fix",
    "fix:css": "stylelint --fix 'playground/**/*.css'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "jest:start": "jest --watchAll",
    "jest:run": "jest",
    "dev": "cd playground && node_modules/.bin/next dev",
    "build": "cd playground && node_modules/.bin/next build",
    "storybook:start": "cd playground && node_modules/.bin/start-storybook -p 6006",
    "storybook:build": "cd playground && node_modules/.bin/build-storybook",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "babel-eslint": "^10.1.0",
    "cypress": "^12.0.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "pinst": "^3.0.0",
    "prettier": "^2.8.0",
    "stylelint": "^14.16.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-order": "^5.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "fs-extra": "^11.1.0",
    "inquirer": "^9.1.4"
  }
}