{
  "name": "with-typescript-types",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "icons:build": "svg-sprite -s --symbol-inline --symbol-dest public --symbol-sprite icons.svg src/assets/icons/*.svg",
    "dev": "yarn run clean && next",
    "build": "yarn run icons:build && next build",
    "start": "next start",
    "type-check": "tsc",
    "storybook:start": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.9.0",
    "@tanstack/react-query-devtools": "^5.9.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "i18next": "^23.7.7",
    "next": "14.0.3",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@storybook/addon-actions": "^7.6.0",
    "@storybook/addon-docs": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/builder-vite": "^7.6.0",
    "@storybook/nextjs": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/ramda": "^0.29.9",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "react-docgen-typescript": "^2.2.2",
    "storybook": "^7.6.0",
    "svg-sprite": "^2.0.2",
    "tailwindcss": "^3.3.5",
    "typescript": "5.3.2",
    "vite": "^5.0.4"
  },
  "license": "MIT"
}
